<!DOCTYPE html>
<link type="text/css" rel="stylesheet" href="http://sites.google.com/site/onlinegames01org/sp-site-homeview.css" />
<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js" type="text/javascript"></script>
<script type="text/javascript">
    function goToView(dest) {
          var supported_views = gadgets.views.getSupportedViews();
          gadgets.views.requestNavigateTo(supported_views[dest]);
        };
    function adjustHeight() { 
        gadgets.window.adjustHeight();
    }
</script>

<div id="ig-homeview-div">
    <div id="thumb-play-button">
        <img id="ig-home-thumb" src="__MSG_thumbnail__" /><img id="expand-down" src="http://sites.google.com/site/onlinegames01org/arrow_expand.png" />
    </div>
</div>
<div id="ig-homeview-content"></div>

<script type="text/javascript">

    var urlParams = gadgets.util.getUrlParameters();
    var language = urlParams['lang'];
    var country = urlParams['country'];

    $('#expand-down').on('click', function() {
        $('#ig-homeview-content').html('<iframe frameborder="0" height="1100px" src="http://www-ig-opensocial.googleusercontent.com/gadgets/ifr?exp_rpc_js=1&exp_track_js=1&url=http%3A%2F%2Fcws-mg.googlecode.com%2Fsvn%2Ftrunk%2Fsp%2Fpt_BR%2Ffl%2Fsp-site.xml&container=ig&view=canvas&lang='+language+'&country='+country+'&sanitize=0&v=5e1e33d6ed1aa906&gadgetId=113595717453088919560" width="100%"></iframe>');
        
        adjustHeight();
    });
    
    gadgets.util.registerOnLoadHandler(adjustHeight);
</script>
]]>
</Content>
